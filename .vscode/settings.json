{
  "cSpell.allowCompoundWords": true,
  "cSpell.logLevel": "Information",
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "csharp",
    "css",
    "diff",
    "dockerfile",
    "git-commit",
    "git-rebase",
    "go",
    "handlebars",
    "html",
    "ignore",
    "ini",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsx-tags",
    "latex",
    "less",
    "markdown",
    "mdx",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "shellscript",
    "text",
    "typescript",
    "typescriptreact",
    "xml",
    "yaml",
    "yml"
  ],
  "cSpell.words": ["docz"],
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "auto-close-tag.activationOnLanguage": [
    "xml",
    "php",
    "blade",
    "ejs",
    "jinja",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "plaintext",
    "markdown",
    "vue",
    "liquid",
    "erb",
    "lang-cfml",
    "cfml",
    "HTML (Eex)",
    "mdx"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.packageManager": "pnpm",
  "eslint.validate": ["javascript", "javascriptreact", "mdx"],
  "nixEnvSelector.nixShellConfig": "${workspaceRoot}/shell.nix"
}
