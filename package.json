  "description": "A 3D reveal animation library for React.",
  "author": "cdfa",
  "license": "SEE LICENSE IN ./LICENSE",
  "repository": "cdfa/react-turn-reveal",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "browserslist": [
    "defaults"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": "^12",
    "npm": "^6",
    "pnpm": "^4.0.0"
