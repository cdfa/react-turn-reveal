{
  "name": "react-turn-reveal",
  "version": "0.1.0",
  "description": "A 3D reveal animation library for React.",
  "author": "cdfa",
  "license": "SEE LICENSE IN ./LICENSE",
  "repository": "cdfa/react-turn-reveal",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5",
    "pnpm": ">=2"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test",
    "test:coverage": "npm run test -- --coverage",
    "build": "rollup -c",
    "dev": "docz dev",
    "deploy": "docz build && gh-pages -d .docz/dist --message \"[skip ci] Updates\"",
    "format": "npm run format:eslint && npm run format:prettier && npm run lint:markdownlint && npm run lint:markdownlint:mdx",
    "format:eslint": "eslint --fix \"{*,src/*,src/**/*}.{js,jsx,json}\"",
    "format:prettier": "prettier --write \"{*,src/*,src/**/*}.mdx\"",
    "lint": "npm run lint:eslint && npm run lint:markdownlint && npm run lint:markdownlint:mdx",
    "lint:eslint": "eslint --max-warnings=0 \"{*,src/*,src/**/*}.{js,jsx,json}\"",
    "lint:markdownlint": "npx markdownlint \"{*,src/*,src/**/*}.md\"",
    "lint:markdownlint:mdx": "npx markdownlint --config .markdownlint-mdxrc \"{*,src/*,src/**/*}.mdx\""
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "docz": "^1.1.0",
    "docz-theme-default": "^1.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.14.0",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-url": "^2.1.0",
    "typescript": "^3.4.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "prop-types": "^15.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{*,src/*,src/**/*}.{js,jsx,json}": "eslint --fix",
    "{*,src/*,src/**/*}.mdx": [
      "prettier write",
      "markdownlint --config .markdownlint-mdxrc"
    ],
    "{*,src/*,src/**/*}.md": "markdownlint",
    "{*,src/*,src/**/*}.{js,jsx}": "git add"
  }
}
